<!--<%= @admin.inspect %>-->
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Editando</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<br>

<div class="row">
  <div class="col-lg-12">

    <% if @admin.errors.any? %>
      <div class="alert alert-danger alert-dismissable">
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">Ã—</button>
          <ul>
            <% @admin.errors.full_messages.each do |message| %>
              <li> <%= message %> </li>
            <% end %>
          </ul>
      </div>
    <% end %>

    <div class="panel panel-default">
        <div class="panel-body">
            <div class="row">
                <div class="col-lg-12">
                  <%= form_with(model: [ :admins_backoffice, @admin], local: true) do |form| %>
                    <div class="form-group">
                      <%= form.label :email %>
                      <%= form.text_field :email, class:"form-control", placeholder:"Digite seu E-mail" %>
                    </div>
                    <div class="form-group">
                      <%= form.label :password %>
                      <%= form.password_field :password, class:"form-control", placeholder:"Digite sua Senha" %>
                    </div>
                    <div class="form-group">
                      <%= form.label :password_confirmation %>
                      <%= form.password_field :password_confirmation, class:"form-control", placeholder:"Digite a Senha Novamente" %>
                    </div>
                    <!-- /.col-lg-6 (nested) -->
                    <button type="submit" class="btn btn-primary">Salvar</button>
                  <% end %>
                </div>
                <!-- /.col-lg-6 (nested) -->
            </div>
            <!-- /.row (nested) -->
        </div>
        <!-- /.panel-body -->
    </div>
    <!-- /.panel -->
  </div>
    <!-- /.col-lg-12 -->
</div>